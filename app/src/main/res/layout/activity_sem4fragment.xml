<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent" android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="UselessParent">
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_sem1fragment"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".fragments.Sem4Fragment">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:layout_marginBottom="10dp"
                android:background="@android:drawable/dialog_holo_light_frame"
                android:layout_weight="1"
                android:baselineAligned="false">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    tools:ignore="NestedWeights">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="160dp"
                        android:id="@+id/java_img"
                        android:scaleType="fitCenter"
                        android:contentDescription="@string/sub"
                        android:src="@drawable/wd"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="top"
                    android:layout_weight="1"
                    >

                    <TextView
                        android:text="@string/notepad_n_java_ds"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/java_name"
                        android:textSize="@dimen/title"
                        android:textStyle="bold"
                        android:layout_margin="@dimen/text_size_large" />


                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dl"
                        android:layout_marginLeft="50dp"
                        android:background="#2ecc71"
                        android:textColor="#ffff"
                        android:onClick="np_download"
                        android:layout_marginBottom="10dp"
                        tools:ignore="RtlHardcoded" />


                </LinearLayout>

            </LinearLayout>



           <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:layout_marginBottom="10dp"
                android:background="@android:drawable/dialog_holo_light_frame"
                android:layout_weight="1"
               android:baselineAligned="false">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    tools:ignore="NestedWeights">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="160dp"
                        android:id="@+id/java1_img"
                        android:scaleType="fitCenter"
                        android:contentDescription="@string/sub"
                        android:src="@drawable/java"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="top"
                    android:layout_weight="1"
                    >

                    <TextView
                        android:text="@string/java_jdk_1_7_n_java_ds"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/java1_name"
                        android:textSize="@dimen/title"
                        android:textStyle="bold"
                        android:layout_margin="@dimen/text_size_large" />


                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dl"
                        android:layout_marginLeft="50dp"
                        android:background="#2ecc71"
                        android:textColor="#ffff"
                        android:onClick="jdk_32bit"
                        android:layout_marginBottom="10dp"
                        tools:ignore="RtlHardcoded" />

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:layout_marginBottom="10dp"
                android:background="@android:drawable/dialog_holo_light_frame"
                android:layout_weight="1"
                android:baselineAligned="false">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    tools:ignore="NestedWeights">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="160dp"
                        android:id="@+id/es1_img"
                        android:contentDescription="@string/sub"
                        android:src="@drawable/es"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="top"
                    android:layout_weight="1"
                    >

                    <TextView
                        android:text="@string/codevision_n_es"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/es_name"
                        android:textSize="@dimen/title"
                        android:textStyle="bold"
                        android:layout_margin="@dimen/text_size_large" />


                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dl"
                        android:layout_marginLeft="50dp"
                        android:background="#2ecc71"
                        android:textColor="#ffff"
                        android:onClick="codevision"
                        android:layout_marginBottom="10dp"
                        tools:ignore="RtlHardcoded" />


                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                tools:ignore="UselessParent"
                android:layout_marginBottom="10dp"
                android:background="@android:drawable/dialog_holo_light_frame"
                android:layout_weight="1"
                android:baselineAligned="false">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    tools:ignore="NestedWeights">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="160dp"
                        android:id="@+id/es_img"
                        android:contentDescription="@string/sub"
                        android:src="@drawable/es"
                        android:scaleType="fitCenter"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="top"
                    android:layout_weight="1"
                    >

                    <TextView
                        android:text="@string/proteus_n_es"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/es1_name"
                        android:textSize="@dimen/title"
                        android:textStyle="bold"
                        android:layout_margin="@dimen/text_size_large" />


                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/dl"
                        android:layout_marginLeft="50dp"
                        android:background="#2ecc71"
                        android:textColor="#ffff"
                        android:onClick="proteus"
                        android:layout_marginBottom="10dp"
                        tools:ignore="RtlHardcoded" />


                </LinearLayout>

            </LinearLayout>


        </LinearLayout>





</android.support.constraint.ConstraintLayout>
        </ScrollView>
    </RelativeLayout>

